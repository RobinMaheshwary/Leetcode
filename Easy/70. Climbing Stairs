class Solution {
public:
    int climbStairs(int n) {
        if(n==1) return 1;
        else if(n==2) return 2;
        else if(n==3) return 3;
        else if(n==4) return 5;
        else if(n==5) return 8;
        else{                                                                                                                                
        return 3*climbStairs(n-5)+5*climbStairs(n-4);
        }
    }
};
